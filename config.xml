<?xml version='1.0' encoding='utf-8'?>
<widget id="foo.bar.cordova.react.issue" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>cordova-react-issue</name>
    <description>
        A sample Apache Cordova + react issue ivestigation which should take a picture and display it.
    </description>
    <author email="sombriks@gmail.com" href="http://sombriks.com.br">
        Sombriks
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <engine name="android" spec="^7.1.4" />
</widget>
